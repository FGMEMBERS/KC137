<PropertyList>
  <name>DU panel</name>
  <w-base>256</w-base>
  <h-base>256</h-base>
  <layers>

    <layer>
      <name>background</name>
      <w>256</w>
      <h>256</h>
      <texture>
        <path>Aircraft/KC135/Instruments/Textures/du-panel.rgb</path>
        <x1>0</x1>
        <y1>0.0</y1>
        <x2>1.0</x2>
        <y2>1.0</y2>
      </texture>
    </layer>

    <layer>
      <name>knob lh</name>
      <w>50</w>
      <h>50</h>
      <texture>
        <path>Aircraft/KC135/Instruments/Textures/du-knob.rgb</path>
        <x1>0</x1>
        <y1>0.0</y1>
        <x2>1.0</x2>
        <y2>1.0</y2>
      </texture>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>-60</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-20</offset>
        </transformation>
        <transformation>
          <type>rotation</type>
          <property>instrumentation/display-unit/control</property>
          <offset>315</offset>
          <interpolation>
            <entry>
              <ind>0</ind>
              <dep>0</dep>
            </entry>
            <entry>
              <ind>1</ind>
              <dep>45</dep>
            </entry>
            <entry>
              <ind>2</ind>
              <dep>90</dep>
            </entry>
            <entry>
              <ind>3</ind>
              <dep>135</dep>
            </entry>
          </interpolation>
        </transformation>
      </transformations>
    </layer>

    <layer>
      <name>knob rh</name>
      <w>50</w>
      <h>50</h>
      <texture>
        <path>Aircraft/KC135/Instruments/Textures/du-knob.rgb</path>
        <x1>0</x1>
        <y1>0.0</y1>
        <x2>1.0</x2>
        <y2>1.0</y2>
      </texture>
      <transformations>
        <transformation>
          <type>x-shift</type>
          <offset>60</offset>
        </transformation>
        <transformation>
          <type>y-shift</type>
          <offset>-20</offset>
        </transformation>
        <transformation>
          <type>rotation</type>
          <property>/instrumentation/radar/minimized</property>
          <interpolation>
            <entry>
              <ind>0</ind>
              <dep>0</dep>
            </entry>
            <entry>
              <ind>1</ind>
              <dep>315</dep>
            </entry>
          </interpolation>
        </transformation>
      </transformations>
    </layer>
  </layers>


  <actions>
    <action>
      <name>lower-normal</name>
      <button>0</button>
      <x>40</x>
      <y>15</y>
      <w>40</w>
      <h>20</h>
      <binding>
        <command>property-assign</command>
        <property>/instrumentation/radar/minimized</property>
        <value>false</value>
      </binding>
    </action>

    <action>
      <name>eng-pri</name>
      <button>0</button>
      <x>0</x>
      <y>0</y>
      <w>40</w>
      <h>20</h>
      <binding>
        <command>property-assign</command>
        <property>/instrumentation/radar/minimized</property>
        <value>true</value>
      </binding>
    </action>
    <action>
      <name>outbd-pfd</name>
      <button>0</button>
      <x>-120</x>
      <y>0</y>
      <w>40</w>
      <h>20</h>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/display-unit/control</property>
        <value>0</value>
      </binding>
    </action>
    <action>
      <name>main-normal</name>
      <button>0</button>
      <x>-80</x>
      <y>15</y>
      <w>40</w>
      <h>20</h>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/display-unit/control</property>
        <value>1</value>
      </binding>
    </action>

    <action>
      <name>inbd-engpri</name>
      <button>0</button>
      <x>-40</x>
      <y>0</y>
      <w>40</w>
      <h>20</h>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/display-unit/control</property>
        <value>2</value>
      </binding>
    </action>

    <action>
      <name>inbd-pfd</name>
      <button>0</button>
      <x>-30</x>
      <y>-30</y>
      <w>40</w>
      <h>20</h>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/display-unit/control</property>
        <value>3</value>
      </binding>
    </action>
  </actions>

</PropertyList>